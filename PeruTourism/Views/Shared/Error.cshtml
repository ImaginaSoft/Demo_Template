@{
    ViewBag.Title = "Peru Torurism - Error";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Contenido -->
<div class="container">
    <div class="row">
        <div class="col-lg-9 col-md-8 col-sm-12 col-xs-12 col-xxs-12">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 col-xxs-12">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 col-xxs-12">
                    <div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 col-xs-8 col-xs-offset-2 col-xxs-12">
                        <div class="col-lg-3 col-md-4 col-xs-4 col-xxs-12 t-center error-ico">
                            <img src="@Url.Content("~/Content/images/error-icon.png")" />
                        </div>
                        <div class="col-lg-9 col-md-8 col-xs-8 col-xxs-12 error-txt">
                            <h2>Se presentó un error inesperado!</h2>
                            <p>Si el problema persiste por favor comunícate con nuestro HELP DESK al número 999 9999.</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>